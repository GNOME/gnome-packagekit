msgid "Stable"
msgstr "स्थिर"

#. TRANSLATORS: A distribution stability level
#: ../src/gpk-enum.c:824
msgid "Unstable"
msgstr "अस्थिर"

#. TRANSLATORS: A distribution stability level
#: ../src/gpk-enum.c:828
msgid "Testing"
msgstr "जाँच रहा है"

#: ../src/gpk-enum.c:845
msgid "A mirror is possibly broken"
msgstr "मिरर संभवतः टूटा है"

#: ../src/gpk-enum.c:848
msgid "The connection was refused"
msgstr "संबंधन अस्वीकारा गया"

#: ../src/gpk-enum.c:851
msgid "The parameter was invalid"
msgstr "पैरामीटर अवैध था"

#: ../src/gpk-enum.c:854
msgid "The priority was invalid"
msgstr "प्राथमिकता अवैध थी"

#: ../src/gpk-enum.c:857
msgid "Backend warning"
msgstr "बैकेंड चेतावनी"

#: ../src/gpk-enum.c:860
msgid "Daemon warning"
msgstr "डेमॉन चेतावनी"

#: ../src/gpk-enum.c:863
msgid "The package list cache is being rebuilt"
msgstr "संकुल सूची फिर निर्मित किया जा रहा है"

#: ../src/gpk-enum.c:866
msgid "An untrusted package was installed"
msgstr "एक भरोसेमंद संकुल संस्थापित था"

#: ../src/gpk-enum.c:869
msgid "A newer package exists"
msgstr "एक नवीनतर संकुल मौजूद है"

#: ../src/gpk-enum.c:872
msgid "Could not find package"
msgstr "संकुल ढ़ूँढ़ नहीं सका"

#: ../src/gpk-enum.c:875
msgid "Configuration files were changed"
msgstr "विन्यास फाइल बदला गया था"

#: ../src/gpk-enum.c:878
msgid "Package is already installed"
msgstr "संकुल पहले से संस्थापित है"

#: ../src/gpk-enum.c:881
msgid "Automatic cleanup is being ignored"
msgstr "स्वचालित सफाई अनदेखा किया गया है"

#. TRANSLATORS: This is when the status is not known
#: ../src/gpk-enum.c:904
msgid "Unknown state"
msgstr "अज्ञात स्थिति"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:908
msgid "Starting"
msgstr "प्रारंभ कर रहा है"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:912
msgid "Waiting in queue"
msgstr "कतार में प्रतीक्षारत"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:916
msgid "Running"
msgstr "चल रहा है"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:920
msgid "Querying"
msgstr "प्रश्न कर रहा है"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:924
msgid "Getting information"
msgstr "सूचना पा रहा है"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:928
msgid "Removing packages"
msgstr "संकुल हटा रहा है"

#. TRANSLATORS: The transaction state
#. TRANSLATORS: The role of the transaction, in present tense
#: ../src/gpk-enum.c:932 ../src/gpk-enum.c:1348
msgid "Downloading packages"
msgstr "संकुल डाउनलोड कर रहा है"

#. TRANSLATORS: The transaction state
#: ../src/gpk-enum.c:940
msgid "Refreshing software list"
msgstr "सॉफ्टवेयर सूची ताजा कर रहा है"

