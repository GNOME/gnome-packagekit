msgid "Unstable"
msgstr "Нестабильный"

#. TRANSLATORS: A distribution stability level
#: ../src/gpk-enum.c:828
msgid "Testing"
msgstr "Тестируемый"

#: ../src/gpk-enum.c:845
msgid "A mirror is possibly broken"
msgstr "Возможно, зеркало неполное"

#: ../src/gpk-enum.c:848
msgid "The connection was refused"
msgstr "Соединение было отклонено"

#: ../src/gpk-enum.c:851
msgid "The parameter was invalid"
msgstr "Неверный параметр"

#: ../src/gpk-enum.c:854
msgid "The priority was invalid"
msgstr "Неверный приоритет"

#: ../src/gpk-enum.c:857
msgid "Backend warning"
msgstr "Предупреждение модуля"

#: ../src/gpk-enum.c:860
msgid "Daemon warning"
msgstr "Предупреждение службы"

#: ../src/gpk-enum.c:863
msgid "The package list cache is being rebuilt"
msgstr "Идёт перестройка кэша списка пакетов"

#: ../src/gpk-enum.c:866
msgid "An untrusted package was installed"
msgstr "Был установлен неподписанный пакет"

#: ../src/gpk-enum.c:869
msgid "A newer package exists"
msgstr "Существует более новый пакет"

#: ../src/gpk-enum.c:872
msgid "Could not find package"
msgstr "Не удалось найти пакет"

#: ../src/gpk-enum.c:875
msgid "Configuration files were changed"
msgstr "Файлы конфигурации были изменены"

#: ../src/gpk-enum.c:878
msgid "Package is already installed"
msgstr "Пакет уже установлен"

#: ../src/gpk-enum.c:881
msgid "Automatic cleanup is being ignored"
msgstr "Автоматическая очистка игнорируется"

