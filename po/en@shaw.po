"𐑞𐑺 𐑦𐑟 𐑯𐑴 𐑯𐑧𐑑𐑢𐑻𐑒 𐑒𐑩𐑯𐑧𐑒𐑖𐑩𐑯 𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑿𐑼 𐑒𐑩𐑯𐑧𐑒𐑖𐑩𐑯 𐑕𐑧𐑑𐑦𐑙𐑟 𐑯 𐑑𐑮𐑲 𐑩𐑜𐑱𐑯."

#: ../src/gpk-enum.c:533
msgid ""
"The package list needs to be rebuilt.\n"
"This should have been done by the backend automatically."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑤𐑦𐑕𐑑 𐑯𐑰𐑛𐑟 𐑑 𐑚𐑰 𐑮𐑰𐑚𐑦𐑤𐑑.\n"
"𐑞𐑦𐑕 𐑖𐑫𐑛 𐑣𐑨𐑝 𐑚𐑰𐑯 𐑛𐑳𐑯 𐑚𐑲 𐑞 𐑚𐑨𐑒𐑧𐑯𐑛 𐑷𐑑𐑩𐑥𐑨𐑑𐑦𐑒𐑤𐑦."

#: ../src/gpk-enum.c:537
msgid ""
"The service that is responsible for handling user requests is out of "
"memory.\n"
"Please restart your computer."
msgstr ""
"𐑞 𐑕𐑻𐑝𐑦𐑕 𐑞𐑨𐑑 𐑦𐑟 𐑮𐑦𐑕𐑐𐑪𐑯𐑕𐑦𐑚𐑩𐑤 𐑓𐑹 𐑣𐑨𐑯𐑛𐑤𐑦𐑙 𐑿𐑟𐑼 𐑮𐑦𐑒𐑢𐑧𐑕𐑑𐑕 𐑦𐑟 𐑬𐑑 𐑝 𐑥𐑧𐑥𐑼𐑦.\n"
"𐑐𐑤𐑰𐑟 𐑮𐑰𐑕𐑑𐑸𐑑 𐑿𐑼 𐑒𐑩𐑥𐑐𐑿𐑑𐑼."

#: ../src/gpk-enum.c:541
msgid "A thread could not be created to service the user request."
msgstr "𐑩 𐑔𐑮𐑧𐑛 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑒𐑮𐑦𐑱𐑑𐑩𐑛 𐑑 𐑕𐑻𐑝𐑦𐑕 𐑞 𐑿𐑟𐑼 𐑮𐑦𐑒𐑢𐑧𐑕𐑑."

#: ../src/gpk-enum.c:552
msgid ""
"A security trust relationship could not be made with software source.\n"
"Please check your security settings."
msgstr ""
"𐑩 𐑕𐑦𐑒𐑘𐑫𐑼𐑦𐑑𐑦 𐑑𐑮𐑳𐑕𐑑 𐑮𐑦𐑤𐑱𐑖𐑩𐑯𐑖𐑦𐑐 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑥𐑱𐑛 𐑢𐑦𐑞 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑿𐑼 𐑕𐑦𐑒𐑘𐑫𐑼𐑦𐑑𐑦 𐑕𐑧𐑑𐑦𐑙𐑟."

#: ../src/gpk-enum.c:556
msgid ""
"The package that is trying to be removed or updated is not already installed."
msgstr "𐑞 𐑐𐑨𐑒𐑦𐑡 𐑞𐑨𐑑 𐑦𐑟 𐑑𐑮𐑲𐑦𐑙 𐑑 𐑚𐑰 𐑮𐑦𐑥𐑵𐑝𐑛 𐑹 𐑳𐑐𐑛𐑱𐑑𐑩𐑛 𐑦𐑟 𐑯𐑪𐑑 𐑷𐑤𐑮𐑧𐑛𐑦 𐑦𐑯𐑕𐑑𐑷𐑤𐑛."

#: ../src/gpk-enum.c:559
msgid ""
"The package that is being modified was not found on your system or in any "
"software source."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑞𐑨𐑑 𐑦𐑟 𐑚𐑰𐑦𐑙 𐑥𐑪𐑛𐑦𐑓𐑲𐑛 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑓𐑬𐑯𐑛 𐑪𐑯 𐑿𐑼 𐑕𐑦𐑕𐑑𐑩𐑥 𐑹 𐑦𐑯 𐑧𐑯𐑦 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕."

#: ../src/gpk-enum.c:562
msgid "The package that is trying to be installed is already installed."
msgstr "𐑞 𐑐𐑨𐑒𐑦𐑡 𐑞𐑨𐑑 𐑦𐑟 𐑑𐑮𐑲𐑦𐑙 𐑑 𐑚𐑰 𐑦𐑯𐑕𐑑𐑷𐑤𐑛 𐑦𐑟 𐑷𐑤𐑮𐑧𐑛𐑦 𐑦𐑯𐑕𐑑𐑷𐑤𐑛."

#: ../src/gpk-enum.c:565
msgid ""
"The package download failed.\n"
"Please check your network connectivity."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑛𐑬𐑯𐑤𐑴𐑛 𐑓𐑱𐑤𐑛.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑿𐑼 𐑯𐑧𐑑𐑢𐑻𐑒 𐑒𐑩𐑯𐑧𐑒𐑑𐑦𐑝𐑦𐑑𐑰."

#: ../src/gpk-enum.c:569
msgid ""
"The group type was not found.\n"
"Please check your group list and try again."
msgstr ""
"𐑞 𐑜𐑮𐑵𐑐 𐑑𐑲𐑐 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑓𐑬𐑯𐑛.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑿𐑼 𐑜𐑮𐑵𐑐 𐑤𐑦𐑕𐑑 𐑯 𐑑𐑮𐑲 𐑩𐑜𐑱𐑯."

#: ../src/gpk-enum.c:573
msgid ""
"The group list could not be loaded.\n"
"Refreshing your cache may help, although this is normally a software source "
"error."
msgstr ""
"𐑞 𐑜𐑮𐑵𐑐 𐑤𐑦𐑕𐑑 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑤𐑴𐑛𐑩𐑛.\n"
"𐑮𐑰𐑓𐑮𐑧𐑖𐑦𐑙 𐑿𐑼 𐑒𐑨𐑖 𐑥𐑱 𐑣𐑧𐑤𐑐, 𐑷𐑤𐑞𐑴 𐑞𐑦𐑕 𐑦𐑟 𐑯𐑹𐑥𐑩𐑤𐑦 𐑩 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕 𐑻𐑼."

#: ../src/gpk-enum.c:578
msgid ""
"A package could not be found that allows the action to complete.\n"
"More information is available in the detailed report."
msgstr ""
"𐑩 𐑐𐑨𐑒𐑦𐑡 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑓𐑬𐑯𐑛 𐑞𐑨𐑑 𐑩𐑤𐑬𐑟 𐑞 𐑨𐑒𐑖𐑩𐑯 𐑑 𐑒𐑩𐑥𐑐𐑤𐑰𐑑.\n"
"𐑥𐑹 𐑦𐑯𐑓𐑼𐑥𐑱𐑖𐑩𐑯 𐑦𐑟 𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤 𐑦𐑯 𐑞 𐑛𐑰𐑑𐑱𐑤𐑛 𐑮𐑩𐑐𐑹𐑑."

#: ../src/gpk-enum.c:582
msgid "The search filter was not correctly formed."
msgstr "𐑞 𐑕𐑻𐑗 𐑓𐑦𐑤𐑑𐑼 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑒𐑩𐑮𐑧𐑒𐑑𐑤𐑦 𐑓𐑹𐑥𐑛."

#: ../src/gpk-enum.c:585
msgid ""
"The package identifier was not well formed when sent to the server.\n"
"This normally indicates an internal error and should be reported."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑲𐑛𐑧𐑯𐑑𐑦𐑓𐑲𐑼 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑢𐑧𐑤 𐑓𐑹𐑥𐑛 𐑢𐑧𐑯 𐑕𐑧𐑯𐑑 𐑑 𐑞 𐑕𐑻𐑝𐑼.\n"
"𐑞𐑦𐑕 𐑯𐑹𐑥𐑩𐑤𐑦 𐑦𐑯𐑛𐑦𐑒𐑱𐑑𐑕 𐑩𐑯 𐑦𐑯𐑑𐑻𐑯𐑩𐑤 𐑻𐑼 𐑯 𐑖𐑫𐑛 𐑚𐑰 𐑮𐑩𐑐𐑹𐑑𐑩𐑛."

#: ../src/gpk-enum.c:589
msgid ""
"An unspecified transaction error has occurred.\n"
"More information is available in the detailed report."
msgstr ""
"𐑩𐑯 𐑩𐑯𐑕𐑐𐑧𐑕𐑩𐑓𐑲𐑛 𐑑𐑮𐑨𐑯𐑟𐑨𐑒𐑖𐑩𐑯 𐑻𐑼 𐑣𐑨𐑟 𐑪𐑒𐑻𐑛.\n"
"𐑥𐑹 𐑦𐑯𐑓𐑼𐑥𐑱𐑖𐑩𐑯 𐑦𐑟 𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤 𐑦𐑯 𐑞 𐑛𐑰𐑑𐑱𐑤𐑛 𐑮𐑩𐑐𐑹𐑑."

#: ../src/gpk-enum.c:593
msgid ""
"The remote software source name was not found.\n"
"You may need to enable an item in Software Sources."
msgstr ""
"𐑞 𐑮𐑦𐑥𐑴𐑑 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕 𐑯𐑱𐑥 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑓𐑬𐑯𐑛.\n"
"𐑿 𐑥𐑱 𐑯𐑰𐑛 𐑑 𐑦𐑯𐑱𐑚𐑩𐑤 𐑩𐑯 𐑲𐑑𐑩𐑥 𐑦𐑯 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕𐑩𐑟."

#: ../src/gpk-enum.c:597
msgid "Removing a protected system package is not allowed."
msgstr "𐑮𐑦𐑥𐑵𐑝𐑦𐑙 𐑩 𐑐𐑮𐑩𐑑𐑧𐑒𐑑𐑩𐑛 𐑕𐑦𐑕𐑑𐑩𐑥 𐑐𐑨𐑒𐑦𐑡 𐑦𐑟 𐑯𐑪𐑑 𐑩𐑤𐑬𐑛."

#: ../src/gpk-enum.c:600
msgid "The action was canceled successfully and no packages were changed."
msgstr "𐑞 𐑨𐑒𐑖𐑩𐑯 𐑢𐑪𐑟 𐑒𐑨𐑯𐑕𐑩𐑤𐑛 𐑕𐑩𐑒𐑕𐑧𐑕𐑓𐑩𐑤𐑦 𐑯 𐑯𐑴 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑢𐑻 𐑗𐑱𐑯𐑡𐑛."

#: ../src/gpk-enum.c:603
msgid ""
"The action was canceled successfully and no packages were changed.\n"
"The backend did not exit cleanly."
msgstr ""
"𐑞 𐑨𐑒𐑖𐑩𐑯 𐑢𐑪𐑟 𐑒𐑨𐑯𐑕𐑩𐑤𐑛 𐑕𐑩𐑒𐑕𐑧𐑕𐑓𐑩𐑤𐑦 𐑯 𐑯𐑴 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑢𐑻 𐑗𐑱𐑯𐑡𐑛.\n"
"𐑞 𐑚𐑨𐑒𐑧𐑯𐑛 𐑛𐑦𐑛 𐑯𐑪𐑑 𐑧𐑜𐑟𐑦𐑑 𐑒𐑤𐑰𐑯𐑤𐑰."

#: ../src/gpk-enum.c:607
msgid ""
"The native package configuration file could not be opened.\n"
"Please make sure configuration is valid."
msgstr ""
"𐑞 𐑯𐑱𐑑𐑦𐑝 𐑐𐑨𐑒𐑦𐑡 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑱𐑖𐑩𐑯 𐑓𐑲𐑤 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑴𐑐𐑩𐑯𐑛.\n"
"𐑐𐑤𐑰𐑟 𐑥𐑱𐑒 𐑖𐑫𐑼 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑱𐑖𐑩𐑯 𐑦𐑟 𐑝𐑨𐑤𐑦𐑛."

#: ../src/gpk-enum.c:611
msgid "The action cannot be canceled at this time."
msgstr "𐑞 𐑨𐑒𐑖𐑩𐑯 𐑒𐑨𐑯𐑪𐑑 𐑚𐑰 𐑒𐑨𐑯𐑕𐑩𐑤𐑛 𐑨𐑑 𐑞𐑦𐑕 𐑑𐑲𐑥."

#: ../src/gpk-enum.c:614
msgid ""
"Source packages are not normally installed this way.\n"
"Check the extension of the file you are trying to install."
msgstr ""
"𐑕𐑹𐑕 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑸 𐑯𐑪𐑑 𐑯𐑹𐑥𐑩𐑤𐑦 𐑦𐑯𐑕𐑑𐑷𐑤𐑛 𐑞𐑦𐑕 𐑢𐑱.\n"
"𐑗𐑧𐑒 𐑞 𐑩𐑒𐑕𐑑𐑧𐑯𐑖𐑩𐑯 𐑝 𐑞 𐑓𐑲𐑤 𐑿 𐑸 𐑑𐑮𐑲𐑦𐑙 𐑑 𐑦𐑯𐑕𐑑𐑷𐑤."

#: ../src/gpk-enum.c:618
msgid ""
"The license agreement was not agreed to.\n"
"To use this software you have to accept the license."
msgstr ""
"𐑞 𐑤𐑲𐑕𐑩𐑯𐑕 𐑩𐑜𐑮𐑰𐑥𐑩𐑯𐑑 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑩𐑜𐑮𐑰𐑛 𐑑.\n"
"𐑑 𐑿𐑕 𐑞𐑦𐑕 𐑕𐑪𐑓𐑑𐑢𐑺 𐑿 𐑣𐑨𐑝 𐑑 𐑨𐑒𐑕𐑧𐑐𐑑 𐑞 𐑤𐑲𐑕𐑩𐑯𐑕."

#: ../src/gpk-enum.c:622
msgid ""
"Two packages provide the same file.\n"
"This is usually due to mixing packages from different software sources."
msgstr ""
"𐑑𐑵 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑐𐑮𐑩𐑝𐑲𐑛 𐑞 𐑕𐑱𐑥 𐑓𐑲𐑤.\n"
"𐑞𐑦𐑕 𐑦𐑟 𐑿𐑠𐑿𐑩𐑤𐑦 𐑛𐑿 𐑑 𐑥𐑦𐑒𐑕𐑦𐑙 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑓𐑮𐑪𐑥 𐑛𐑦𐑓𐑼𐑩𐑯𐑑 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕𐑩𐑟."

#: ../src/gpk-enum.c:626
msgid ""
"Multiple packages exist that are not compatible with each other.\n"
"This is usually due to mixing packages from different software sources."
msgstr ""
"𐑥𐑳𐑤𐑑𐑦𐑐𐑩𐑤 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑧𐑒𐑟𐑦𐑕𐑑 𐑞𐑨𐑑 𐑸 𐑯𐑪𐑑 𐑒𐑩𐑥𐑐𐑨𐑑𐑩𐑚𐑩𐑤 𐑢𐑦𐑞 𐑰𐑗 𐑳𐑞𐑼.\n"
"𐑞𐑦𐑕 𐑦𐑟 𐑿𐑠𐑿𐑩𐑤𐑦 𐑛𐑿 𐑑 𐑥𐑦𐑒𐑕𐑦𐑙 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑓𐑮𐑪𐑥 𐑛𐑦𐑓𐑼𐑩𐑯𐑑 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕𐑩𐑟."

#: ../src/gpk-enum.c:630
msgid ""
"There was a (possibly temporary) problem connecting to a software source.\n"
"Please check the detailed error for further details."
msgstr ""
"𐑞𐑺 𐑢𐑪𐑟 𐑩 (𐑐𐑪𐑕𐑩𐑚𐑩𐑤𐑦 𐑑𐑧𐑥𐑐𐑼𐑼𐑦) 𐑐𐑮𐑪𐑚𐑤𐑩𐑥 𐑒𐑩𐑯𐑧𐑒𐑑𐑦𐑙 𐑑 𐑩 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑞 𐑛𐑰𐑑𐑱𐑤𐑛 𐑻𐑼 𐑓𐑹 𐑓𐑻𐑞𐑼 𐑛𐑰𐑑𐑱𐑤𐑟."

#: ../src/gpk-enum.c:634
msgid ""
"Failed to initialize packaging backend.\n"
"This may occur if other packaging tools are being used simultaneously."
msgstr ""
"𐑓𐑱𐑤𐑛 𐑑 𐑦𐑯𐑦𐑖𐑩𐑤𐑲𐑟 𐑐𐑨𐑒𐑦𐑡𐑦𐑙 𐑚𐑨𐑒𐑧𐑯𐑛.\n"
"𐑞𐑦𐑕 𐑥𐑱 𐑪𐑒𐑻 𐑦𐑓 𐑳𐑞𐑼 𐑐𐑨𐑒𐑦𐑡𐑦𐑙 𐑑𐑵𐑤𐑟 𐑸 𐑚𐑰𐑦𐑙 𐑿𐑟𐑛 𐑕𐑲𐑥𐑩𐑤𐑑𐑱𐑯𐑰𐑩𐑕𐑤𐑰."

#: ../src/gpk-enum.c:638
msgid ""
"Failed to close down the backend instance.\n"
"This error can normally be ignored."
msgstr ""
"𐑓𐑱𐑤𐑛 𐑑 𐑒𐑤𐑴𐑕 𐑛𐑬𐑯 𐑞 𐑚𐑨𐑒𐑧𐑯𐑛 𐑦𐑯𐑕𐑑𐑨𐑯𐑕.\n"
"𐑞𐑦𐑕 𐑻𐑼 𐑒𐑨𐑯 𐑯𐑹𐑥𐑩𐑤𐑦 𐑚𐑰 𐑦𐑜𐑯𐑹𐑛."

#: ../src/gpk-enum.c:642
msgid ""
"Cannot get the exclusive lock on the packaging backend.\n"
"Please close any other legacy packaging tools that may be open."
msgstr ""
"𐑒𐑨𐑯𐑪𐑑 𐑜𐑧𐑑 𐑞 𐑧𐑒𐑕𐑒𐑤𐑵𐑕𐑦𐑝 𐑤𐑪𐑒 𐑪𐑯 𐑞 𐑐𐑨𐑒𐑦𐑡𐑦𐑙 𐑚𐑨𐑒𐑧𐑯𐑛.\n"
"𐑐𐑤𐑰𐑟 𐑒𐑤𐑴𐑟 𐑧𐑯𐑦 𐑳𐑞𐑼 𐑤𐑧𐑜𐑩𐑕𐑰 𐑐𐑨𐑒𐑦𐑡𐑦𐑙 𐑑𐑵𐑤𐑟 𐑞𐑨𐑑 𐑥𐑱 𐑚𐑰 𐑴𐑐𐑩𐑯."

#: ../src/gpk-enum.c:646
msgid "None of the selected packages could be updated."
msgstr "𐑯𐑳𐑯 𐑝 𐑞 𐑕𐑩𐑤𐑧𐑒𐑑𐑩𐑛 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑒𐑫𐑛 𐑚𐑰 𐑳𐑐𐑛𐑱𐑑𐑩𐑛."

#: ../src/gpk-enum.c:649
msgid "The repository configuration could not be modified."
msgstr "𐑞 𐑮𐑰𐑐𐑭𐑟𐑩𐑑𐑪𐑮𐑰 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑱𐑖𐑩𐑯 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑥𐑪𐑛𐑦𐑓𐑲𐑛."

#: ../src/gpk-enum.c:652
msgid ""
"Installing the local file failed.\n"
"More information is available in the detailed report."
msgstr ""
"𐑦𐑯𐑕𐑑𐑷𐑤𐑦𐑙 𐑞 𐑤𐑴𐑒𐑩𐑤 𐑓𐑲𐑤 𐑓𐑱𐑤𐑛.\n"
"𐑥𐑹 𐑦𐑯𐑓𐑼𐑥𐑱𐑖𐑩𐑯 𐑦𐑟 𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤 𐑦𐑯 𐑞 𐑛𐑰𐑑𐑱𐑤𐑛 𐑮𐑩𐑐𐑹𐑑."

#: ../src/gpk-enum.c:656
msgid "The package security signature could not be verified."
msgstr "𐑞 𐑐𐑨𐑒𐑦𐑡 𐑕𐑦𐑒𐑘𐑫𐑼𐑦𐑑𐑦 𐑕𐑦𐑜𐑯𐑩𐑗𐑼 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑝𐑧𐑮𐑦𐑓𐑲𐑛."

#: ../src/gpk-enum.c:659
msgid ""
"The package security signature is missing and this package is untrusted.\n"
"This package was not signed when created."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑕𐑦𐑒𐑘𐑫𐑼𐑦𐑑𐑦 𐑕𐑦𐑜𐑯𐑩𐑗𐑼 𐑦𐑟 𐑥𐑦𐑕𐑦𐑙 𐑯 𐑞𐑦𐑕 𐑐𐑨𐑒𐑦𐑡 𐑦𐑟 untrusted.\n"
"𐑞𐑦𐑕 𐑐𐑨𐑒𐑦𐑡 𐑢𐑪𐑟 𐑯𐑪𐑑 𐑕𐑲𐑯𐑛 𐑢𐑧𐑯 𐑒𐑮𐑦𐑱𐑑𐑩𐑛."

#: ../src/gpk-enum.c:663
msgid "Repository configuration was invalid and could not be read."
msgstr "𐑮𐑰𐑐𐑭𐑟𐑩𐑑𐑪𐑮𐑰 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑱𐑖𐑩𐑯 𐑢𐑪𐑟 𐑦𐑯𐑝𐑨𐑤𐑦𐑛 𐑯 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑮𐑧𐑛."

#: ../src/gpk-enum.c:666
msgid ""
"The package you are attempting to install is not valid.\n"
"The package file could be corrupt, or not a proper package."
msgstr ""
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑿 𐑸 𐑩𐑑𐑧𐑥𐑐𐑑𐑦𐑙 𐑑 𐑦𐑯𐑕𐑑𐑷𐑤 𐑦𐑟 𐑯𐑪𐑑 𐑝𐑨𐑤𐑦𐑛.\n"
"𐑞 𐑐𐑨𐑒𐑦𐑡 𐑓𐑲𐑤 𐑒𐑫𐑛 𐑚𐑰 𐑒𐑼𐑳𐑐𐑑, 𐑹 𐑯𐑪𐑑 𐑩 𐑐𐑮𐑪𐑐𐑼 𐑐𐑨𐑒𐑦𐑡."

#: ../src/gpk-enum.c:670
msgid ""
"Installation of this package prevented by your packaging system's "
"configuration."
msgstr "𐑦𐑯𐑕𐑑𐑩𐑤𐑱𐑖𐑩𐑯 𐑝 𐑞𐑦𐑕 𐑐𐑨𐑒𐑦𐑡 𐑐𐑮𐑦𐑝𐑧𐑯𐑑𐑩𐑛 𐑚𐑲 𐑿𐑼 𐑐𐑨𐑒𐑦𐑡𐑦𐑙 𐑕𐑦𐑕𐑑𐑩𐑥'𐑕 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑱𐑖𐑩𐑯."

#: ../src/gpk-enum.c:673
msgid ""
"The package that was downloaded is corrupt and needs to be downloaded again."
msgstr "𐑞 𐑐𐑨𐑒𐑦𐑡 𐑞𐑨𐑑 𐑢𐑪𐑟 𐑛𐑬𐑯𐑤𐑴𐑛𐑩𐑛 𐑦𐑟 𐑒𐑼𐑳𐑐𐑑 𐑯 𐑯𐑰𐑛𐑟 𐑑 𐑚𐑰 𐑛𐑬𐑯𐑤𐑴𐑛𐑩𐑛 𐑩𐑜𐑱𐑯."

#: ../src/gpk-enum.c:676
msgid ""
"All of the packages selected for install are already installed on the system."
msgstr "𐑷𐑤 𐑝 𐑞 𐑐𐑨𐑒𐑦𐑡𐑩𐑟 𐑕𐑩𐑤𐑧𐑒𐑑𐑩𐑛 𐑓𐑹 𐑦𐑯𐑕𐑑𐑷𐑤 𐑸 𐑷𐑤𐑮𐑧𐑛𐑦 𐑦𐑯𐑕𐑑𐑷𐑤𐑛 𐑪𐑯 𐑞 𐑕𐑦𐑕𐑑𐑩𐑥."

#: ../src/gpk-enum.c:679
msgid ""
"The specified file could not be found on the system.\n"
"Check the file still exists and has not been deleted."
msgstr ""
"𐑞 𐑕𐑐𐑧𐑕𐑦𐑓𐑲𐑛 𐑓𐑲𐑤 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑓𐑬𐑯𐑛 𐑪𐑯 𐑞 𐑕𐑦𐑕𐑑𐑩𐑥.\n"
"𐑗𐑧𐑒 𐑞 𐑓𐑲𐑤 𐑕𐑑𐑦𐑤 𐑧𐑒𐑟𐑦𐑕𐑑𐑕 𐑯 𐑣𐑨𐑟 𐑯𐑪𐑑 𐑚𐑰𐑯 𐑛𐑦𐑤𐑰𐑑𐑩𐑛."

#: ../src/gpk-enum.c:683
msgid ""
"Required data could not be found on any of the configured software sources.\n"
"There were no more download mirrors that could be tried."
msgstr ""
"𐑮𐑦𐑒𐑢𐑲𐑼𐑛 𐑛𐑱𐑑𐑩 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑓𐑬𐑯𐑛 𐑪𐑯 𐑧𐑯𐑦 𐑝 𐑞 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑛 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕𐑩𐑟.\n"
"𐑞𐑺 𐑢𐑻 𐑯𐑴 𐑥𐑹 𐑛𐑬𐑯𐑤𐑴𐑛 𐑥𐑦𐑮𐑼𐑟 𐑞𐑨𐑑 𐑒𐑫𐑛 𐑚𐑰 𐑑𐑮𐑲𐑛."

#: ../src/gpk-enum.c:687
msgid ""
"Required upgrade data could not be found in any of the configured software "
"sources.\n"
"The list of distribution upgrades will be unavailable."
msgstr ""
"𐑮𐑦𐑒𐑢𐑲𐑼𐑛 𐑳𐑐𐑜𐑮𐑱𐑛 𐑛𐑱𐑑𐑩 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑓𐑬𐑯𐑛 𐑦𐑯 𐑧𐑯𐑦 𐑝 𐑞 𐑒𐑩𐑯𐑓𐑦𐑜𐑘𐑼𐑛 𐑕𐑪𐑓𐑑𐑢𐑺 𐑕𐑹𐑕𐑩𐑟.\n"
"𐑞 𐑤𐑦𐑕𐑑 𐑝 𐑛𐑦𐑕𐑑𐑮𐑦𐑚𐑿𐑖𐑩𐑯 𐑩𐑐𐑜𐑮𐑱𐑛𐑟 𐑢𐑦𐑤 𐑚𐑰 𐑩𐑯𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤."

#: ../src/gpk-enum.c:691
msgid ""
"The package that is trying to be installed is incompatible with this system."
msgstr "𐑞 𐑐𐑨𐑒𐑦𐑡 𐑞𐑨𐑑 𐑦𐑟 𐑑𐑮𐑲𐑦𐑙 𐑑 𐑚𐑰 𐑦𐑯𐑕𐑑𐑷𐑤𐑛 𐑦𐑟 𐑦𐑯𐑒𐑩𐑥𐑐𐑨𐑑𐑩𐑚𐑩𐑤 𐑢𐑦𐑞 𐑞𐑦𐑕 𐑕𐑦𐑕𐑑𐑩𐑥."

#: ../src/gpk-enum.c:694
msgid ""
"There is insufficient space on the device.\n"
"Free some space on the system disk to perform this operation."
msgstr ""
"𐑞𐑺 𐑦𐑟 𐑦𐑯𐑕𐑩𐑓𐑦𐑖𐑩𐑯𐑑 𐑕𐑐𐑱𐑕 𐑪𐑯 𐑞 𐑛𐑦𐑝𐑲𐑕.\n"
"𐑓𐑮𐑰 𐑕𐑳𐑥 𐑕𐑐𐑱𐑕 𐑪𐑯 𐑞 𐑕𐑦𐑕𐑑𐑩𐑥 𐑛𐑦𐑕𐑒 𐑑 𐑐𐑼𐑓𐑹𐑥 𐑞𐑦𐑕 𐑪𐑐𐑼𐑱𐑖𐑩𐑯."

#: ../src/gpk-enum.c:698
msgid "Additional media is required to complete the transaction."
msgstr "𐑩𐑛𐑦𐑖𐑩𐑯𐑩𐑤 𐑥𐑰𐑛𐑦𐑩 𐑦𐑟 𐑮𐑦𐑒𐑢𐑲𐑼𐑛 𐑑 𐑒𐑩𐑥𐑐𐑤𐑰𐑑 𐑞 𐑑𐑮𐑨𐑯𐑟𐑨𐑒𐑖𐑩𐑯."

#: ../src/gpk-enum.c:701
msgid ""
"You have failed to provide correct authentication.\n"
"Please check any passwords or account settings."
msgstr ""
"𐑿 𐑣𐑨𐑝 𐑓𐑱𐑤𐑛 𐑑 𐑐𐑮𐑩𐑝𐑲𐑛 𐑒𐑩𐑮𐑧𐑒𐑑 𐑷𐑔𐑧𐑯𐑑𐑦𐑒𐑱𐑖𐑩𐑯.\n"
"𐑐𐑤𐑰𐑟 𐑗𐑧𐑒 𐑧𐑯𐑦 𐑐𐑭𐑕𐑢𐑼𐑛𐑟 𐑹 𐑩𐑒𐑬𐑯𐑑 𐑕𐑧𐑑𐑦𐑙𐑟."

#: ../src/gpk-enum.c:705
msgid ""
"The specified update could not be found.\n"
"It could have already been installed or no longer available on the remote "
"server."
msgstr ""
"𐑞 𐑕𐑐𐑧𐑕𐑦𐑓𐑲𐑛 𐑳𐑐𐑛𐑱𐑑 𐑒𐑫𐑛 𐑯𐑪𐑑 𐑚𐑰 𐑓𐑬𐑯𐑛.\n"
"𐑦𐑑 𐑒𐑫𐑛 𐑣𐑨𐑝 𐑷𐑤𐑮𐑧𐑛𐑦 𐑚𐑰𐑯 𐑦𐑯𐑕𐑑𐑷𐑤𐑛 𐑹 𐑯𐑴 𐑤𐑪𐑙𐑜𐑼 𐑩𐑝𐑱𐑤𐑩𐑚𐑩𐑤 𐑪𐑯 𐑞 𐑮𐑦𐑥𐑴𐑑 𐑕𐑻𐑝𐑼."
