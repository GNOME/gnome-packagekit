msgid "A mirror is possibly broken"
msgstr "鏡像站台可能損毀"

#: ../src/gpk-enum.c:710
msgid "The connection was refused"
msgstr "連線被拒絕"

#: ../src/gpk-enum.c:713
msgid "The parameter was invalid"
msgstr "此參數是無效的"

#: ../src/gpk-enum.c:716
msgid "The priority was invalid"
msgstr "此優先權是無效的"

#: ../src/gpk-enum.c:719
msgid "Backend warning"
msgstr "後端程式警示"

#: ../src/gpk-enum.c:722
msgid "Daemon warning"
msgstr "伺服程式警示"

#: ../src/gpk-enum.c:725
msgid "The package list cache is being rebuilt"
msgstr "套件清單快取正在重建"

#: ../src/gpk-enum.c:728
msgid "An untrusted package was installed"
msgstr "已安裝一個不信任的套件"

#: ../src/gpk-enum.c:731
